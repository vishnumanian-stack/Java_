import java.io.*;
import java.util.*;

public class Solution {
    public void add(int x,int y)
    {
        int c=x+y;
        System.out.println(c);
    }


    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
     
        int a=sc.nextInt();
        int b=sc.nextInt();
        Solution s1=new Solution();
        s1.add(a,b);
    }  
    }
